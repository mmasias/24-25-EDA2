    + Pixel[][] devolverPixeles()
}

class Pixel {
    - Color color
    + Pixel()
    + Color devolverColor()
}

class Color {
    - char representacion
    + Color()
    + char devolverRepresentacion()
}

class Grafica {
    - final int tasaHZ
    - Cola colaFrames
    + Grafica()
    - void generarFrames()
    + Frame[] devolverDosFrames()
}

class Pantalla {
    - Frame[] frames
    - Grafica grafica
    + void mostrar()
}

class Main {
    + main(String[] args)
}

